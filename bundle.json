      "rom": "",
      "ram": "",
      "deps": {
        "components": [
          "ability_base",
          "ability_runtime",
          "access_token",
          "bundle_framework",
          "device_auth",
          "distributeddatamgr",
          "dsoftbus",
          "eventhandler",
          "hisysevent_native",
          "hitrace_native",
          "hiviewdfx_hilog_native",
          "ipc",
          "multimedia_image_standard",
          "napi",
          "os_account",
          "safwk",
          "samgr_standard",
          "startup_l2",
          "utils_base"
        ],
        "third_party": []
      },
      "build": {

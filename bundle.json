              ]
            },
            "name": "//foundation/distributedschedule/dmsfwk/interfaces/innerkits/continuation_manager:continuation_manager"
          },
          {
            "header": {
              "header_base": "//foundation/distributedschedule/dmsfwk/interfaces/innerkits/common/include",
              "header_files": [
                "distributed_ability_manager_client.h",
                "distributed_ability_manager_proxy.h"
              ]
            },
            "name": "//foundation/distributedschedule/dmsfwk/interfaces/innerkits/common:common_sdk"
          }
        ],
        "test": [
          "//foundation/distributedschedule/dmsfwk/services/dtbschedmgr:unittest",
          "//foundation/distributedschedule/dmsfwk/services/dtbabilitymgr:unittest"
        ]
      }
